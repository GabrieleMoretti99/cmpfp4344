//Vector3D.cpp

#include "Vector3D.h"
#include <iostream>
using namespace std;

int main () {

  double sandro;

  Vector3D v1 = Vector3D();
  
  v1.setVector3D(1,2,3);
  v1.magnitude();
  v1.theta();
  v1.phi();
  cout << "Vettore v1:" << endl;
  v1.print();

  Vector3D v2 = Vector3D();
  v2.setVector3D(4,5,6);
  v2.magnitude();
  v2.theta();
  v2.phi();
  cout << "\nVettore v2:" << endl;
  v2.print();

  sandro = v1.scalar(v2);
  cout << "Prodotto scalare = "<< sandro << endl;
  

  return 0;

}

